<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/Login_ForeColor</item>
        <item name="colorPrimaryDark">@color/Login_ForeColor</item>
        <item name="android:navigationBarColor">#fafafa</item>
        <item name="android:navigationBarDividerColor">@color/Home_act_text</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>

    <style name="AppThemeNoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:navigationBarColor">#fafafa</item>
        <item name="android:navigationBarDividerColor">@color/Home_act_text</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
</resources>